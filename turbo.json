{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env",
    "tsconfig.json"
  ],
  "pipeline": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "db:studio": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**"
      ]
    },
    "db:generate": {
      "inputs": [
        "src"
      ],
      "outputs": [
        "drizzle/**"
      ],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "ADMIN_EMAIL",
    "CI",
    "DATABASE_HOST",
    "DATABASE_PASSWORD",
    "DATABASE_USERNAME",
    "EDGE_CONFIG",
    "KV_REST_API_READ_ONLY_TOKEN",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "KV_URL",
    "NEXT_PUBLIC_APP_DESCRIPTION",
    "NEXT_PUBLIC_APP_DOMAIN",
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_PUSHER_CLUSTER",
    "NEXT_PUBLIC_PUSHER_KEY",
    "NEXT_PUBLIC_STRIPE_PRO_50K_MONTHLY_PLAN_ID",
    "NEXT_PUBLIC_STRIPE_PRO_50K_YEARLY_PLAN_ID",
    "NEXT_PUBLIC_STRIPE_PRO_UNLIMITED_MONTHLY_PLAN_ID",
    "NEXT_PUBLIC_STRIPE_PRO_UNLIMITED_YEARLY_PLAN_ID",
    "AUTH_SECRET",
    "AUTH_URL",
    "NODE_ENV",
    "OPENAI_API_KEY",
    "PUSHER_APP_ID",
    "PUSHER_SECRET",
    "RESEND_API_KEY",
    "SKIP_ENV_VALIDATION",
    "SMTP_FROM",
    "SMTP_HOST",
    "SMTP_PASSWORD",
    "SMTP_PORT",
    "SMTP_USER",
    "STRIPE_API_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "VERCEL_API_URL",
    "VERCEL_BEARER_TOKEN",
    "VERCEL_ENABLE_DOMAIN",
    "VERCEL_PROJECT_ID",
    "VERCEL_TEAM_ID",
    "VERCEL_URL",
    "VERCEL"
  ]
}