{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "pipeline": {
    "db:generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:push": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "outputs": [
        ".next/**",
        ".expo/**"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    }
  },
  "globalEnv": [
    "CI",
    "DATABASE_URL",
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_URL",
    "DATABASE_URL",
    "EDGE_CONFIG",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_APP_DOMAIN",
    "VERCEL_API_URL",
    "VERCEL_BEARER_TOKEN",
    "VERCEL_ENABLE_DOMAIN",
    "VERCEL_PROJECT_ID",
    "VERCEL_TEAM_ID",
    "SMTP_HOST",
    "SMTP_PORT",
    "SMTP_USER",
    "SMTP_PASSWORD",
    "SMTP_FROM",
    "STRIPE_API_KEY",
    "STRIPE_PRO_10K_MONTHLY_PLAN_ID",
    "STRIPE_PRO_10K_YEARLY_PLAN_ID",
    "STRIPE_PRO_UNLIMITED_MONTHLY_PLAN_ID",
    "STRIPE_PRO_UNLIMITED_YEARLY_PLAN_ID",
    "STRIPE_WEBHOOK_SECRET"
  ]
}