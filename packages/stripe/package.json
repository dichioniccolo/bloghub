  "devDependencies": {
    "@acme/eslint-config": "^0.1.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@acme/eslint-config/base"
